    "from bokeh.plotting import figure, show\n",
    "\n",
    "height = [-0.5, 1.0, -1.5]\n",
    "\n",
    "x = [1,2,3]\n",
    "y = [x / 2. for x in height]\n",
    "\n",
    "p = figure(width=300, height=300, tools=\"hover\")\n",
    "p.rect(x=x, y=y, width=0.8, height=height)\n",
    "\n",
    "show(p)"
   ]
  }
